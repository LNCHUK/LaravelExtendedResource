{
    "name": "lnchuk/laravel-extended-resource",
    "description": "A Laravel package to provide extended resource routes, including deleted records, restoring records and confirmation of deletes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Lynch",
            "email": "tom@lnch.co.uk"
        }
    ],
    "require": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "LNCHUK\\LaravelExtendedResource\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LNCHUK\\LaravelExtendedResource\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "5.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LNCHUK\\LaravelExtendedResource\\LaravelExtendedResourceServiceProvider"
            ]
        }
    }
}
